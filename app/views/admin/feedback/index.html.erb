<div class="row">
  <div class="col-md-12">
    <div class="col-md-1">
      <h4>View feedback</h4>
    </div>
    <div class="col-md-4">
      <h3> Subject </h3>
    </div>
    <div class="col-md-2">
      <h3> First Name </h3>
    </div>
    <div class="col-md-2">
      <h3> Last Name </h3>
    </div>
    <div class="col-md-2 col-md-push-1">
      <h3> Email </h3>
    </div>
  </div>
<% @feedbacks.all.each do |feedback| %>
  <div class="col-md-12">
    <div class="col-md-1">
      <%= link_to( "View", admin_feedback_path(feedback.id)) %>
    </div>
    <div class="col-md-4">
      <%= feedback.subject %>
    </div>
    <div class="col-md-2">
      <%= feedback.first_name %>
    </div>
    <div class="col-md-2">
      <%= feedback.last_name %>
    </div>
    <div class="col-md-2">
      <%= feedback.email_address %>
    </div>
    <div class="col-md-1">
      <%= link_to "Delete", admin_feedback_path(feedback.id), method: :delete %>
    </div>
  </div>
<% end %>
</div>
